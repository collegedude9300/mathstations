<?xml version="1.0" encoding="UTF-8"?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms" xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" x:Class="MathStations.DivLevThree">
    <ContentPage.Content>
        <StackLayout Margin="20,10,10,20">
        <Frame BackgroundColor="SandyBrown" Padding="2" CornerRadius="0">
            <Label Text="Division Page!" HorizontalTextAlignment="Center" TextColor="Black" FontSize="24" />
        </Frame>
    <Label Text="Division Level 3: (with remainders)"> </Label>
            <Button Text="Question 1"
           Clicked="ProbOne_DivLevThree" />
             <Label x:Name="prob1lev3div" />
         <Button Text="Question 2"
            Clicked="ProbTwo_DivLevThree" />
              <Label x:Name="prob2lev3div" />
         <Button Text="Question 3"
           Clicked="ProbThree_DivLevThree" />
             <Label x:Name="prob3lev3div" />
         <Button Text="Question 4"
            Clicked="ProbFour_DivLevThree" />
              <Label x:Name="prob4lev3div" />           
    <Label Text="Click on the next level, once you're ready, or if you want to go back to the homepage, click, 'Back to Home'."/>
    <Button x:Name="buttonDivFour" Text="Level 4" Clicked="DivFour" VerticalOptions="Center"/>
    <Button x:Name="buttonHome" Text="Back to Home" Clicked="BackToHomeClicked" VerticalOptions="Center"/>
    </StackLayout>
    </ContentPage.Content>
</ContentPage>
