<?xml version="1.0" encoding="UTF-8"?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms" xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" x:Class="MathStations.AddLevTwo">

<ContentPage.Content>
        <StackLayout Margin="20,10,10,20">
        <Frame BackgroundColor="PeachPuff" Padding="2" CornerRadius="0">
            <Label Text="Addition Page!" HorizontalTextAlignment="Center" TextColor="Black" FontSize="24" />
        </Frame>
    <Label Text="Addition Level 2:"> </Label>
               <Button Text="Question 1"
           Clicked="ProbOne_AddLevTwo" />
             <Label x:Name="prob1lev2add" />
         <Button Text="Question 2"
            Clicked="ProbTwo_AddLevTwo" />
              <Label x:Name="prob2lev2add" />
         <Button Text="Question 3"
           Clicked="ProbThree_AddLevTwo" />
             <Label x:Name="prob3lev2add" />
         <Button Text="Question 4"
            Clicked="ProbFour_AddLevTwo" />
              <Label x:Name="prob4lev2add" />            
    <Label Text="Click on the next level, once you're ready, or if you want to go back to the homepage, click, 'Back to Home'."/>
    <Button x:Name="buttonAddThree" Text="Level 3" Clicked="AddThree" VerticalOptions="Center"/>
    <Button x:Name="buttonHome" Text="Back to Home" Clicked="BackToHomeClicked" VerticalOptions="Center"/>
    </StackLayout>
    </ContentPage.Content>




       
</ContentPage>
